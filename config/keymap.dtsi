/ {
  keymap {
    compatible = "zmk,keymap";

    default_layer {
      bindings = 
        <&kp ESC>, <&kp Q>, <&kp W>, <&kp E>, <&kp R>, <&kp T>, <&encoder1>,
        <&kp Y>, <&kp U>, <&kp I>, <&kp O>, <&kp P>, <&kp BSPC>,
        <&kp TAB>, <&kp A>, <&kp S>, <&kp D>, <&kp F>, <&kp G>,
        <&kp H>, <&kp J>, <&kp K>, <&kp L>, <&kp ENTER>,
        <&kp LSFT>, <&kp Z>, <&kp X>, <&kp C>, <&kp V>, <&kp B>,
        <&kp N>, <&kp M>, <&kp COMMA>, <&kp DOT>, <&kp SLASH>, <&encoder2>,
        <&kp LCTRL>, <&kp LALT>, <&kp HOME>, <&studio_unlock>, <&kp CONVERT>,
        <&kp LEFT>, <&kp DOWN>, <&kp RIGHT>;
    };
  };
};

&studio_unlock {
  bindings = <&kp SPACE>;
};

